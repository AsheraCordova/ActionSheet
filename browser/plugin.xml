<?xml version="1.0" encoding="utf-8"?>
<plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
        id="com.ashera.actionsheet.web"
        version="1.0">

  <name>CordovaActionSheetPlugin</name>

  <engines>
    <engine name="cordova" version=">=3.4.0"/>
  </engines>
  
  <platform name="browser">
    <config-file target="config.xml" parent="/*">
      <feature name="CordovaActionSheetPlugin">
        <param name="android-package" value="com.ashera.actionsheet.CordovaActionSheetPlugin"/>
        <param name="onload" value="true" />
      </feature>
    </config-file>
	<framework src="core.gradle" custom="true" type="gradleReference" />
	<asset src="www/css/actionsheet.css" target="css/actionsheet.css" />
   	<js-module src="www/js/add_res.js" name="actionsheetAddRes">
    	 <runs/>
  	</js-module>
   	 <!-- start src -->
     <!-- src end -->
     <!-- start res -->
     <!-- res end -->
     <!-- start tsc -->
     <!-- tsc end -->
  </platform>


</plugin>